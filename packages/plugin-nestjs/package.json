{
  "name": "ssr-plugin-nestjs",
  "version": "6.2.26",
  "description": "plugin-nestjs for ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json \" \" tsc -p ./tsconfig.esm.json\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangyuang/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangyuang/ssr/issues"
  },
  "homepage": "https://github.com/zhangyuang/ssr#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "ora": "^4.0.4",
    "rimraf": "^3.0.2",
    "ssr-common-utils": "^6.2.10",
    "semver": "^7.3.5",
    "shelljs": "^0.8.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72"
  },
  "peerDependencies": {
    "@types/shelljs": "^0.8.11",
    "@types/semver": "^7.3.9",
    "@nestjs/common": ">=7.5.1",
    "@nestjs/core": ">=7.5.1",
    "reflect-metadata": ">=0.1.13",
    "rxjs": ">=6.6.3"
  },
  "devDependencies": {
    "ssr-types": "^6.2.3"
  }
}
